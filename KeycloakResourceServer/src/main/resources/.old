server.port=8083
#spring.security.oauth2.resourceserver.jwt.issuer-uri = http://localhost:8080/auth/realms/ms-users/
spring.security.oauth2.resourceserver.jwt.jwk-set-uri = http://localhost:8080/auth/realms/padotec/protocol/openid-connect/certs

rabbitmq.host = localhost
rabbitmq.port: 5672
rabbitmq.username: guest
rabbitmq.password: guest
rabbitmq.listener.simple.retry.enabled: true
rabbitmq.listener.simple.retry.initial-interval: 3s
rabbitmq.listener.simple.retry.max-attempts: 6
rabbitmq.listener.simple.retry.max-interval: 10s
rabbitmq.listener.simple.retry.multiplier: 2

spring.devtools.restart.poll-interval=2s
spring.devtools.restart.quiet-period=1s
